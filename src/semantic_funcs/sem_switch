def sem_wtf_switch(lexeme, line, symbol_table, semanticResult):
    # track cases and defaults
    seen_cases = set()
    seen_default = False
    in_case_block = False
    literals = ['Void Literal', 'Type Literal', 'TROOF Literal', 'NUMBAR Literal', 'NUMBR Literal', 'YARN Literal']

    def check_case_literal(case_value):
        """Ensure the case value is a valid literal."""
        if case_value[1] not in literals:
            return f"semantic error at line {line + 1}: Invalid case value '{case_value[0]}'\n"
        return None

    if lexeme[0][0] == "WTF?":
        # start of WTF block, no semantic errors here
        return semanticResult

    if lexeme[0][0] == "OMG":
        if seen_default:
            semanticResult += f"semantic error at line {line + 1}: 'OMG' cannot appear after 'OMGWTF'\n"
            return semanticResult

        case_value = lexeme[1]
        error = check_case_literal(case_value)
        if error:
            semanticResult += error
            return semanticResult

        if case_value[0] in seen_cases:
            semanticResult += f"semantic error at line {line + 1}: Duplicate case value '{case_value[0]}'\n"
            return semanticResult

        seen_cases.add(case_value[0])
        in_case_block = True
        return semanticResult

    if lexeme[0][0] == "OMGWTF":
        if seen_default:
            semanticResult += f"semantic error at line {line + 1}: Multiple 'OMGWTF' defaults are not allowed\n"
            return semanticResult

        seen_default = True
        in_case_block = True
        return semanticResult

    if lexeme[0][0] == "GTFO":
        if not in_case_block:
            semanticResult += f"semantic error at line {line + 1}: 'GTFO' must appear within a case block\n"
            return semanticResult
        return semanticResult

    if lexeme[0][0] == "OIC":
        # End of the WTF block
        if not seen_cases and not seen_default:
            semanticResult += f"semantic error at line {line + 1}: 'WTF?' block must have at least one 'OMG' or 'OMGWTF' case\n"
            return semanticResult
        return semanticResult

    # invalid lexeme in the WTF block
    semanticResult += f"semantic error at line {line + 1}: Unexpected syntax '{lexeme[0][0]}' in 'WTF?' block\n"
    return semanticResult
